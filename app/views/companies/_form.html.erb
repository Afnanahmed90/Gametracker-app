<%= form_for @company do |f| %>
    <div class="input-field col s6">
        <%= f.label :name, 'Company Name'%>
        <%= f.text_field :name%>
    </div>
    <div class="input-field col s6">
        <%= f.label :platform, 'Platform used' %>
        <%= f.text_field :platform %>
    </div>
    <div class="input-field col s6">
        <%= f.label :logo, 'Logo' %>
        <%= f.text_field :logo %>
    </div>
    <%= f.submit "Submit", :class => "btn waves-effect waves-light" %>
  <% end %>